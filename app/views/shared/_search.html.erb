<div class="search_wrapper">
  <% @search.each do |a| %>
    <div class="search_item_wrapper">
      <div class="search_image">
        <%= link_to image_tag(a.photo.url(:icon)), show_user_path(a) %>
      </div>
      <div class="search_name">
        <%= link_to username(a), show_user_path(a) %>
      </div>
    <% end %>
  </div>
</div>


